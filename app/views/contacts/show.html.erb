<% contact = @contact %>
<header>
  <div class="container">
    <h1><%= contact.display_name %></h1>
    <nav>
      <%= link_to 'Edit', edit_contact_path(contact), :class => 'button' %>
      <%= link_to 'Delete',
                  contact_path(contact),
                  :class => 'button',
                  :data => { :confirm => 'Are you sure?' },
                  :method => :delete %>
      <%= link_to 'Back', return_url(entity_path(contact.entity)) %>
    </nav>
  </div>
</header>
<section class="container">
  <dl>
    <dt>Title</dt>
    <dd><%= contact.title %></dd>
  </dl>
  <dl>
    <dt>First Name</dt>
    <dd><%= contact.first_name %></dd>
  </dl>
  <dl>
    <dt>Last Name</dt>
    <dd><%= contact.last_name %></dd>
  </dl>
  <dl>
    <dt>Display Name</dt>
    <dd><%= contact.display_name %></dd>
  </dl>
  <dl>
    <dt>Email Address</dt>
    <dd><%= contact.email_address %></dd>
  </dl>
  <dl>
    <dt>Fax Number</dt>
    <dd><%= contact.fax_number %></dd>
  </dl>
  <dl>
    <dt>Mobile Number</dt>
    <dd><%= contact.mobile_number %></dd>
  </dl>
  <dl>
    <dt>Phone Number</dt>
    <dd><%= contact.phone_number %></dd>
  </dl>
  <dl>
    <dt>UUID</dt>
    <dd><%= contact.uuid %></dd>
  </dl>
</section>